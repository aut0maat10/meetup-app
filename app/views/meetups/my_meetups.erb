
<% if Helpers.is_logged_in?(session)%>
    <h1>My Meetups</h1>
        <br><br>
    <% @meetups.each do |meetup|%>
        <h3><a href="/meetups/<%=meetup.id%>"><%= meetup.name %> </a><br></h3>
        <br>
        <h4><%= meetup.time%></h4>
        <br><br>
        <p><%= meetup.description%></p>
        <br>
        <p><%= meetup.location%></p>
        <br><br>
    <% end %>
    
    <h1>My RSVPs</h1>
        <br><br>
        
    <% @my_meetups.each do |meetup|%>
        
        <h3><a href="/meetups/<%=meetup.id%>"><%= meetup.name %> </a><br></h3>
        <br>
        <h4><%= meetup.time%></h4>
        <br><br>
        <p><%= meetup.description%></p>
        <br>
        <p><%= meetup.location%></p>
        <br><br>
    <% end %>
<% end %>

<a href="/meetups"><< Back to Meetups Index</a>
